    "import pyperclip\n",
    "def get_clipboard():\n",
    "    cc = pyperclip.paste()\n",
    "    if isinstance(cc, str):\n",
    "        return cc\n",
    "    else:\n",
    "        return \"Nothing is there in clipboard\""
